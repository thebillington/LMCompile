<BLOCK> : <EXPRESSION> | <COMPARISON> | <FUNCTIONCALL>

<COMPARISON> ::= <RESERVEDWORD> <OPENPARENTHESIS> <EXPRESSION> <CLOSEPARENTHESIS> <OPENBRACE> <BLOCK> <CLOSEBRACE>

<FUNCTIONCALL> ::= <RESERVEDWORD> <OPENPARENTHESIS> <EXPRESSION> <CLOSEPARENTHESIS> <SEMICOLON>

<EXPRESSION> ::= <OPENPARENTHESIS> <EXPRESSION> <CLOSEPARENTHESIS>
                | <EXPRESSION> <OPERATOR> <EXPRESSION>
                | <IDENTIFIER> 
                | <CONSTANT>

<OPERATOR> ::= <ASSIGNMENTOPERATOR> | <UNARYOPERATOR> | <COMPOUNDOPERATOR>

<CONSTANT> ::= <INTCON> | <CHARCON> | <STRINGCON>